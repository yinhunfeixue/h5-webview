{
  "name": "h5-webview",
  "version": "3.0.6",
  "description": "h5版，仿原生view管理器",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "publicNpm": "npm run build && npm login && npm version patch && npm publish",
    "public": "npm run deploy && npm run publicNpm"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yinhunfeixue/h5-webview.git"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "es",
    "lib"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "core-js": "^3.8.1",
    "react": "^16.12.0"
  },
  "sideEffects": true,
  "resolutions": {
    "@types/react": "^16.14.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/react": "^16.14.2",
    "@types/vfile-message": "^2.0.0",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.1.0-rc.7",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  }
}
